{
  "name": "decidim-session-ui",
  "version": "1.0.0",
  "description": "Session UI for the Decidim project.",
  "license": "GPL-3.0",
  "author": "stalker <stalker@ttc.io>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/decidim/training.git"
  },
  "cleanData": "rm -r public/data",
  "cleanSite": "rm -r _site",
  "scripts": {
    "start": "node scripts/start.js",
    "metalsmith": "babel-node metalsmith.js",
    "build": "npm run metalsmith -s && node scripts/build.js",
    "build-data": "npm run metalsmith -s",
    "build-ui": "touch public/index.html && node scripts/build.js",
    "clean-data": "$npm_package_cleanData",
    "clean-site": "$npm_package_cleanSite",
    "clean": "$npm_package_cleanData && $npm_package_cleanSite",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "ajv": "^5.0.1-beta.3",
    "baconjs": "^0.7.84",
    "bluebird": "^3.3.4",
    "bootstrap": "^3.3.7",
    "es6-promise": "^3.1.2",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "history": "2.0.0",
    "humanize-duration": "^3.7.1",
    "lodash": "^4.13.1",
    "marked": "^0.3.5",
    "metalsmith-transmark": "^0.1.0",
    "moment": "^2.13.0",
    "moment-duration-format": "^1.3.0",
    "node-sass": "^4.5.2",
    "path": "^0.12.7",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-router": "^2.3.0",
    "request": "^2.71.0",
    "sass-loader": "^6.0.3",
    "transmark": "0.1.5",
    "winston": "^2.3.1",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "^6.10.1",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react-app": "^2.2.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.20.0",
    "brfs": "^1.4.3",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs": "0.0.1-security",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "metalsmith": "^2.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-multi-language": "^0.2.0",
    "metalsmith-paths": "^2.1.2",
    "metalsmith-writemetadata": "^0.4.5",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "react-widgets": "^3.4.6",
    "style-loader": "0.13.1",
    "transform-loader": "^0.2.4",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
